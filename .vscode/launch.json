{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet: build",
      "program": "${workspaceFolder}/machineservice.server/bin/Debug/net8.0/machineservice.server.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole",
      "env": {
        "ASPNETCORE_URLS": "https://localhost:5003",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ENVIRONMENT__MachineName": "debug-machine",
        "ENVIRONMENT__IsProd": "false",
        "IPBLACKLIST__STORAGE": "file://.?pathmapped=true",
        "IPBLACKLIST__ENTRY": "ipblacklist-example.json",
        "DATABASE__CONNECTIONSTRING": "", // Set these in local.environmentvariables.json if needed
        "MESSAGING__CONNECTIONSTRING": ""
      }
    }
  ]
}
